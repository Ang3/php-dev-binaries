{
  "name": "ang3/php-dev-binaries",
  "type": "binary",
  "description": "PHP project binaries for developpers",
  "keywords": ["php", "dev", "bin", "linter", "check", "code", "phpstan", "php-cs-fixer"],
  "license": "MIT",
  "authors": [
    {
      "name": "Joanis ROUANET",
      "email": "joanis.ang3@gmail.com",
      "homepage": "https://github.com/Ang3"
    }
  ],
  "require": {
    "php": ">=7.1",
    "friendsofphp/php-cs-fixer": ">=2.14",
    "phpstan/phpstan": ">=0.9",
    "phpstan/phpstan-doctrine": ">=0.9",
    "phpstan/phpstan-symfony": ">=0.9"
  },
  "bin": [
    "bin/dev_check_code.sh",
    "bin/dev_fix_code.sh",
    "bin/symfony/sf_clear_cache.sh",
    "bin/symfony/sf_clear_doctrine_cache.sh",
    "bin/symfony/sf_clear_logs.sh",
    "bin/symfony/sf_reset.sh"
  ],
  "config": {
    "sort-packages": true
  }
}
